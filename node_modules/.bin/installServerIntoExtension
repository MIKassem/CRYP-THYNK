#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules/vscode-languageserver/bin/node_modules:/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules/vscode-languageserver/node_modules:/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules:/home/azz/Documents/vscode/EECE455/project2/node_modules:/home/azz/Documents/vscode/EECE455/node_modules:/home/azz/Documents/vscode/node_modules:/home/azz/Documents/node_modules:/home/azz/node_modules:/home/node_modules:/node_modules:/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules/vscode-languageserver/bin/node_modules:/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules/vscode-languageserver/node_modules:/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules:/home/azz/Documents/vscode/EECE455/project2/node_modules:/home/azz/Documents/vscode/EECE455/node_modules:/home/azz/Documents/vscode/node_modules:/home/azz/Documents/node_modules:/home/azz/node_modules:/home/node_modules:/node_modules:/home/azz/Documents/vscode/EECE455/project2/cryptotutor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
